# A sample Gemfile
source 'https://rubygems.org'

ruby '2.2.3'

gem 'sinatra'
gem 'sinatra-contrib'
gem 'ably'
gem 'pg'
gem 'sequel'
gem 'puma'

gem 'dotenv', :groups => [:development, :test]

gem 'rack-test', :require => 'rack/test'

group :test do
  gem 'minitest', :require => 'minitest/autorun'
  gem 'minitest-reporters'
end
